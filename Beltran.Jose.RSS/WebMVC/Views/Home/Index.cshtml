@model RssLibrary.Channel

@{
    ViewBag.Title = "Home Page";
}

<div id="poster" class="jumbotron">
    <h1>Welcome to Microsft Channel 9 Rss</h1>
</div>

<h2 style="text-decoration: underline;">Current Feeds in Channel</h2>


<table id="feeds" class="table table-striped table-responsive">
    <tbody>
        <tr class="row">
            <td style="text-align: center"><strong>Date</strong></td>
            <td style="text-align: center"><strong>Title</strong></td>
            <td style="text-align: center"><strong>Go to...</strong></td>
        </tr>
        @foreach (var elem in Model.Items)
        {
            <tr class="row">
                <td>@elem.PubDate</td>
                <td style="padding-left: 10px;">@elem.Title</td>
                <td style="text-align: center"><a href="@elem.Link"><img src="../../Content/external_link.png" /></a></td>
            </tr>
        }
    </tbody>
</table>

<script>
    setActiveMenuItem();  
</script>